{
  "name": "@zxcore/smart-antd-vue3ui",
  "version": "1.0.5",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "vite",
    "build": "tsx scripts/gen-components.ts && vite build && tsx scripts/build-umd.ts",
    "tags": "node scripts/release-tag.cjs"
  },
  "type": "module",
  "main": "lib/index.cjs",
  "module": "es/index.mjs",
  "types": "es/index.d.ts",
  "style": "es/styles/index.css",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./style": "./es/styles/index.css",
    "./styles": "./es/styles/index.css",
    "./styles/*": "./dist/styles/*",
    "./hooks": {
      "types": "./es/hooks/index.d.ts",
      "import": "./es/hooks/index.mjs",
      "require": "./lib/hooks/index.cjs"
    },
    "./utils": {
      "types": "./es/utils/index.d.ts",
      "import": "./es/utils/index.mjs",
      "require": "./lib/utils/index.cjs"
    },
    "./style.css": "./es/styles/index.css",
    "./dist/style.css": "./dist/style.css",
    "./resolver": {
      "types": "./es/resolver.d.ts",
      "import": "./es/resolver.mjs",
      "require": "./lib/resolver.cjs"
    },
    "./es": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./lib": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./es/*": "./es/*",
    "./lib/*": "./lib/*",
    "./src/*": "./src/*"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "ant-design-vue": ">=4.0.0",
    "vue": ">=3.2.0",
    "vue-request": "^2.0.4"
  },
  "devDependencies": {
    "ant-design-vue": "^4.0.0"
  },
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "typesVersions": {
    "*": {
      "resolver": [
        "es/resolver.d.ts"
      ],
      "hooks": [
        "es/hooks/index.d.ts"
      ],
      "utils": [
        "es/utils/index.d.ts"
      ],
      "es/*": [
        "es/*"
      ],
      "lib/*": [
        "lib/*"
      ]
    }
  },
  "sideEffects": [
    "**/*.css",
    "**/*.less",
    "**/style/*",
    "**/style.ts",
    "**/style.mjs",
    "**/style.cjs",
    "src/style.ts"
  ]
}